{
	"env": {
		"browser": false,
		"commonjs": true,
		"es6": true,
		"node": true,
		"mocha": true
	},
	"parserOptions": {
		"ecmaVersion": 11,
		// "ecmaFeatures": {
		// 	"jsx": true,
		// },
		"sourceType": "module"
	},
	"rules": {
		"no-const-assign": "warn",
		"no-undef": "warn",
		"no-unreachable": "off",
		"no-unused-vars": "warn",
		"valid-typeof": "warn"
	}
}
