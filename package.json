{
 "name": "launch-config",
 "displayName": "Launch Configs",
 "description": "Create shortcuts to launch specific debug configurations.",
 "publisher": "ArturoDent",
 "version": "0.6.0",
 "license": "MIT",
 "repository": {
  "type": "git",
  "url": "https://github.com/ArturoDent/launch-config"
 },
 "engines": {
  "vscode": "^1.50.0"
 },
 "categories": [
  "Debuggers"
 ],
 "keywords": [
  "launch",
  "debug",
  "keybinding"
 ],
 "activationEvents": [
  "onStartupFinished",
  "onCommand:launches.showAllLaunchConfigs"
 ],
 "main": "./src/extension.js",
 "contributes": {
  "commands": [
   {
    "command": "launches.showAllLaunchConfigs",
    "title": "Launch Configs: Show all launch configurations"
   },
   {
    "command": "L.editor.action.copyLinesDownAction",
    "title": "?my copy lines down title"
   }
  ],
  "configuration": {
   "type": "object",
   "title": "Launches Debug Configuration",
   "properties": {
    "launches": {}
   }
  }
 },
 "scripts": {
  "lint": "eslint .",
  "pretest": "npm run lint",
  "test": "node ./test/runTest.js"
 },
 "devDependencies": {
  "@types/glob": "^7.1.1",
  "@types/mocha": "^7.0.2",
  "@types/node": "^13.11.0",
  "@types/vscode": "^1.50.0",
  "eslint": "^6.8.0",
  "glob": "^7.1.6",
  "mocha": "^7.1.2",
  "typescript": "^3.8.3",
  "vscode-test": "^1.3.0"
 }
}